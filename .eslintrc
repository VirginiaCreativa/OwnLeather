{
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
        "global-require": 0,
        "no-fallthrough": "off"
    }
}
